{
	"$schema": "http://json-schema.org/draft-04/schema#",
	"type": "object",
	"properties": {
		"id": {
			"name": "id",
			"type": "string",
			"pattern": "^[0-9a-zA-Z_-]+$"
		},
		"name": {
			"name": "Exercise",
			"type": "string"
		},
		"force": {
			"name": "Force",
			"type": [
				"string",
				"null"
			],
			"enum": [
				null,
				"static",
				"pull",
				"push"
			]
		},
		"level": {
			"name": "Level",
			"type": "string",
			"enum": [
				"beginner",
				"intermediate",
				"expert"
			]
		},
		"mechanic": {
			"name": "Mechanic",
			"type": [
				"string",
				"null"
			],
			"enum": [
				"isolation",
				"compound",
				null
			]
		},
		"equipment": {
			"name": "Equipment",
			"type": [
				"string",
				"null"
			],
			"enum": [
				null,
				"medicine ball",
				"dumbbell",
				"body only",
				"bands",
				"kettlebells",
				"foam roll",
				"cable",
				"machine",
				"barbell",
				"exercise ball",
				"e-z curl bar",
				"other"
			]
		},
		"primaryMuscles": {
			"name": "Primary Muscles",
			"type": "array",
			"items": {
				"type": "string",
				"enum": [
					"abdominals",
					"abductors",
					"adductors",
					"biceps",
					"calves",
					"chest",
					"forearms",
					"glutes",
					"hamstrings",
					"lats",
					"lower back",
					"middle back",
					"neck",
					"quadriceps",
					"shoulders",
					"traps",
					"triceps"
				]
			}
		},
		"secondaryMuscles": {
			"name": "Secondary Muscles",
			"type": "array",
			"items": {
				"type": "string",
				"enum": [
					"abdominals",
					"abductors",
					"adductors",
					"biceps",
					"calves",
					"chest",
					"forearms",
					"glutes",
					"hamstrings",
					"lats",
					"lower back",
					"middle back",
					"neck",
					"quadriceps",
					"shoulders",
					"traps",
					"triceps"
				]
			}
		},
		"instructions": {
			"name": "Instructions",
			"type": "array",
			"items": [
				{
					"type": "string"
				}
			]
		},
		"category": {
			"name": "Category",
			"type": "string",
			"enum": [
				"powerlifting",
				"strength",
				"stretching",
				"cardio",
				"olympic weightlifting",
				"strongman",
				"plyometrics"
			]
		},
		"images": {
			"name": "Images",
			"type": "array",
			"items": [
				{
					"type": "string"
				}
			]
		}
	},
	"required": [
		"id",
		"name",
		"level",
		"mechanic",
		"equipment",
		"primaryMuscles",
		"secondaryMuscles",
		"instructions",
		"category",
		"images"
	]
}